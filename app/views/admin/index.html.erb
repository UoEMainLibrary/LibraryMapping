<div id="main-container">
  <nav class="bg-blue">
    <div class="container-fluid">
        <div class="navbar-header">
          <img class="logo" src="/assets/logo-01.png"><h2>Library Maps | Feedback</h2>
        </div>
   </div>
  </nav>
<div class="container text-center">
  <div class="row">
    <h5>User feedback</h5>
    <div class="statistic col-sm-4">
      <div class="statistic-graphic blue">
        <span class="glyphicon glyphicon-user"></span>
      </div>
      <div class="statistic-text">
        <div class="statistic-title">Total</div>
        <div class="text-center"><%= @total %></div>
      </div>
    </div>
    <div class="statistic col-sm-4">
      <div class="statistic-graphic green">
        <span class="glyphicon glyphicon-ok"></span>
      </div>
      <div class="statistic-text">
        <div class="statistic-title">Found</div>
        <div class="text-center"><%= @found %></div>
      </div>
    </div>
    <div class="statistic col-sm-4">
      <div class="statistic-graphic orange">
        <span class="glyphicon glyphicon-remove"></span>
      </div>
      <div class="statistic-text">
        <div class="statistic-title">Not Found</div>
        <div class="text-center"><%= @total - @found %></div>
      </div>
    </div>
  </div>
  <div class="linkblock row">
    <%= link_to "Main Library", {controller: "admin", action: "map", floor: 1, library: "main"}, data: { no_turbolink: true, floor: 1 }, :class=>"links col-sm-6" %>
  <%= link_to "Murray Library", {controller: "admin", action: "map", floor: 1, library: "murray"}, data: { no_turbolink: true, floor: 1 },  :class=>"links col-sm-6"%>
  </div>
  <div class="row">
    <h5>Messages</h5>
    <table class="table table-bordered">
      <thead style="background-color: #1B325E; color:#fff;">
        <td>NAME</td>
        <td>EMAIL</td>
        <td>MESSAGE</td>
      </thead>
      <tbody>
      <% @feedback_messages.each do |feedback| %>
          <tr>
            <td><%= feedback.name %></td>
            <td><%= feedback.email %></td>
            <td><%= feedback.message %></td>
          </tr>
      <%end%>
      </tbody>
    </table>
</div>

  
</div>

</div>